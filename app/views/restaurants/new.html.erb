<h1>Add a new Restaurant</h1>
<div class="container">
  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :category %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.submit %>
  <% end %>
  <%= link_to "Back to the list", restaurants_path %>
</div>
